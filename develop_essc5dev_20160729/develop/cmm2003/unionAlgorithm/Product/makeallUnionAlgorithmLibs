# Wolfgang Wang, 2004/12/07

HOMEDIR=$CMM2003DIR/unionAlgorithm

clear
echo 开始编译 unionAlgorithm 所有库
echo 按回车键继续
read anykey

clear
echo 编译 unionAlgorithm 1.0 版本
echo 按回车键继续
read anykey
cd $HOMEDIR/src
make
echo ""
echo unionAlgorithm 1.0版本编译结束
echo 按回车键继续
read anykey

clear
echo 编译 unionAlgorithm 20050706.withoutBaseDes 版本
echo 按回车键继续
read anykey
cd $HOMEDIR/src
mk20050706.withoutBaseDes
echo ""
echo unionAlgorithm 20050706.withoutBaseDes 版本编译结束
echo 按回车键继续
read anykey

clear
echo 编译 unionAlgorithm 结束
echo 按回车键继续
read anykey


